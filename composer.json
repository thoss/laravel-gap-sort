{
  "name": "thoss/laravel-gap-sort",
  "description": "Package to sort items with laravel",
  "keywords": [
    "sort",
    "sortable",
    "eloquent",
    "model",
    "laravel",
    "behaviour",
    "gap",
    "performant"
  ],
  "authors": [
    {
      "name": "Timm Hossfeld"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/database": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "spatie/laravel-package-tools": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Thoss\\GapSort\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thoss\\GapSort\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Thoss\\GapSort\\GapSortServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "orchestra/testbench": "^8.5"
  }
}
